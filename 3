");});$translate('descriptions.search_market',{locationName:locationName}).then(function(translationString){$rootScope.metadata.description=translationString;},function(){$rootScope.metadata.description="Looking for the best bars, brunch spots, and everything in between? Search our curated collections for ".concat(locationName," restaurants and book them right on Resy.");});}function loadNearbyTranslation(){$translate('nearby').then(function(translation){vm.nearbyTranslation=translation;}).catch(angular.noop);}function showAuthModal(){$rootScope.$broadcast('MODAL','signin');}function onLocationSelected(e,location,previousLocation){if(previousLocation){vm.previousLocation=previousLocation;}if(location){if(location.code!==$routeParams.location||location.translationKey==='nearby'){$route.updateParams({location:location.code});}else if(previousLocation&&previousLocation.translationKey==='nearby'){$route.reload();}// Reset, but only if location is already set (not on initial load)
